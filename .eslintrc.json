{
  "extends": [
    "next",
    "prettier",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "no-undefined": "error",
    "no-duplicate-imports":"error",
    "@typescript-eslint/comma-spacing":"warn",
    "space-in-parens": "warn",
    "func-call-spacing": "warn",
    "quotes": "off",
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off"
  }
}
